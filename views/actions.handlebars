<div class="row">
    <h1>Actions</h1>
    
        <table>
            <tr>
                <th>Types</th>
                <th>Price</th>
                <th>When</th>
            </tr>
            {{#each actions}}
            <tr>
                <td>{{type}}</td>
                <td>{{cost}}</td>
                <td>{{actionTimestamp}}</td>
            </tr>
            {{/each}}
        </table>
</div>